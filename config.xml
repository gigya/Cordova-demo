<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-facebook4" spec="git+https://github.com/gigya/cordova-plugin-facebook4.git">
        <variable name="APP_ID" value="626108487583023" />
        <variable name="APP_NAME" value="Gigya test App" />
    </plugin>
    <plugin name="cordova-plugin-gigya" spec="/Users/victornavarro/Downloads/cordova-plugin-gigya-master">
        <variable name="GOOGLE_IOS_CLIENT_ID" value="883545813749-fohgj471c9l8ufukasio1b87jqfgtvps.apps.googleusercontent.com" />
    </plugin>
    <plugin name="cordova-plugin-googleplus" spec="git+https://github.com/gigya/cordova-plugin-googleplus.git">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.10464331611-auvpsaiih61j8r5m8u52bvv37ro2m115" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
